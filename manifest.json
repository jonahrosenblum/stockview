{
"manifest_version": 2,
    "name": "stockview",
    "description": "testing a possible extension",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*",
                "*://*/*"
            ],
            "js": [
                "thirdParty/jquery-3.3.1.js",
                "page.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
